{
    "version":1,
    "disable_existing_loggers":false,
    "formatters":{
        "simple":{
            "format":"%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers":{
        "file":{
            "class":"logging.FileHandler",
            "filename":"log/uvicorn.log",
            "formatter":"simple"
        }
    },
    "loggers":{
        "uvicorn.access":{
            "level":"INFO",
            "handlers":["file"]
        },
        "uvicorn":{
            "level":"INFO",
            "handlers":["file"]
        }
    }
}